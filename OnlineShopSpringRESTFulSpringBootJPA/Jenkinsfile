node
{
    stage("git clone")
    {
        git "https://github.com/SaiPranavReddy/OnlineShopRepo.git"

    }
    dir("OnlineShopSpringRESTFulSpringBootJPA/"){
        stage("mvn clean")
        {
            bat "mvn clean"
        }
        stage("sonar analysis")
        {
            withSonarQubeEnv("SONAR_HOME")
            {
                bat "mvn sonar:sonar"
            }
        }
        stage("mvn package")
        {
            bat "mvn package"
        }
        
    }
    
}
